     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PURPOSE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Anonymisieren von Personen und Vertragsdaten</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FEATURES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Abhängig von der gewählten Verarbeitungsart werden Personen- und</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Vertragsdaten anonymisiert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Verarbeitungsart: Standard mit eigener Selektion Gewünschte</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Geschäftspartner-Nr. werden im Selektionsbild erfasst. Diese</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Geschäftspartner werden gemäss gewähltem Umfang der Anonymisierung</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>anonymisiert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Verarbeitungsart: Verarbeitung Triggertabelle Die von BUTOS abgeholten</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Löschkandidaten in der Triggertabelle ZVCD_DSG_TRIGGER werden gemäss</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>gewähltem Umfang der Anonymisierung anonymisiert. Bereits anonymisierte</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Daten dürfen kein 2. Mal anonymisiert werden. Dies wird mittels Prüfen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>der Einträge in Tabelle ZVCD_DSG_TRIGGER gemacht.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Es ist eine Prüfung eingebaut, dass das Programm mit der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Verarbeitungsart &quot;Standard mit eigener Selektion&quot; auf dem</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Produktivsystem nicht ausgeführt werden kann.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Es ist eine Prüfung eingebaut, dass eine Person nicht mehrmals</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>anonymisiert werden kann.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Spezielle Regeln</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>BUT0ID: Einträge mit TYPE ZV_SOZ und ZV_AHV des Partners werden gelöscht</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>BUT0BK: Nur diejenigen BUT0BK Einträge anonymisieren, welche dem zu</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>anonymisierenden Partner zugeordnet sind. Somit sollen keine mehrfach</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>verwendete Zahlstellen anonymisiert werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>TIBAN: Nur diejenigen BUT0BK Einträge anonymisieren, welche dem zu</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>anonymisierenden Partner zugeordnet sind.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>DFKKOP: Bei Auszahlungsbelegen wird die Referenz zur Zahlstelle gelöscht</TDLINE>
      </TLINE>
